{
  "manifest_version": 3,
  "name": "Remotify",
  "description": "A lightweight spotify session inside your browser. Spotify Premium required.",
  "version": "1.1",
  "action": {
    "default_popup": "/popup/remotify.html"
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "128": "/images/128.png"
  }
}
